<h3>Upcoming NBA Events: </h3>
    <table>
      <thead>
        <tr>
          <td>As Of</td>
          <td>Home </td>
          <td>Away </td>
          <td>Event Date </td>
          <td>Min Price ($) </td>
          <td>Tix Remaining </td>
          <td>Venue </td>
          <td>City </td>
        </tr>
      </thead>
    <% @all_most_recent.each do |c| %>
      <tbody>
        <%= render :partial => "event_recent", :locals => {:c => c } %>
        <% @all_less_recent.find_all_by_event_type(c.event_type).each do |r| %>
        <%= render :partial => "event_older", :locals => {:r => r }%>
        <% end %>
    </tbody>
  <% end %>
  </table>